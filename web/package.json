{
  "name": "web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "wasm": "wasm-pack build --dev --target web ../backend",
    "wasm-release": "wasm-pack build --release --target web ../backend",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
