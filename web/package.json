    "@types/geojson": "^7946.0.15",
    "@types/node": "^22.10.2",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.2",
    "showdown": "^2.1.0",
    "svelte": "^4.2.12",
    "svelte-check": "^4.1.1",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2",
    "vite": "^4.5.2",
    "vite-plugin-wasm-pack": "0.1.11"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
